##初始一个服务器
initialize: ip=10.10.0.63, port=10001, instreams=1, outstreams=1;

set_payload_header: type=1234, mbu=56, mch=78, jc1=9A, jc2=BC;
set_payload_body: length=20, contents="Hello Sctplib";

##Wait for an association(连接)
wait_for_assoc;

LOOP: times = 100;
    send_chunks: num=2000, stream=0, delay=0;
    pause: time=5;
ENDLOOP;

shutdown;


